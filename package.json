{
  "name": "mockingoose",
  "version": "2.14.0",
  "description": "A Jest package for mocking mongoose models",
  "main": "./lib",
  "scripts": {
    "test": "jest",
    "build": "babel src -d lib -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/alonronin/mockingoose.git"
  },
  "keywords": [
    "jest",
    "mock",
    "mongoose"
  ],
  "author": "Alon Valadji",
  "license": "The Unlicense",
  "bugs": {
    "url": "https://github.com/alonronin/mockingoose/issues"
  },
  "homepage": "https://github.com/alonronin/mockingoose#readme",
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "jest": "^24.8.0",
    "prettier": "^1.18.2"
  },
  "peerDependencies": {
    "mongoose": ">=4.9.10"
  },
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "testRegex": ".test.js$"
  },
  "engines": {
    "node": ">=6.4.0"
  }
